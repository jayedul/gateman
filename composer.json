{
    "name": "devjk/gateman",
    "license": "gpl-v3",
    "autoload": {
        "psr-4": {
      		"DevJK\\SLR\\": "classes/"
		}
    },
    "scripts": {
        "lint": "phpcs --standard=WordPress --extensions=php -s ./classes ./templates",
        "lint:fix": "phpcbf --standard=WordPress --extensions=php ./classes ./templates"
    },
	"require": {
		"devjk/wp-toolkit": "dev-main"
	},
    "require-dev": {
        "10up/phpcs-composer": "dev-master",
        "wp-coding-standards/wpcs": "^2.3"
    },
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/jayedul/wordpress-toolkit"
		}
	],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
